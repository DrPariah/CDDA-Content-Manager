                    { "$or": [
                        { "jsonObject.symbol": { "$exists": true } },
                        { "jsonObject.copy-from": { "$exists": true } }
                    ] },
                    { "$or": [
                        { "jsonObject.type": { "$eq": "terrain" } },
                        { "jsonObject.type": { "$eq": "furniture" } },
                        { "jsonObject.type": { "$eq": "vehicle_part" } }
