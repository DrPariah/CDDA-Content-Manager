{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": true,
    "title": "Overmap land use code",
    "type": "object",
    "id": "https://niebert.github.io/json-editor",
    "options": {
        "disable_collapse": false,
        "disable_edit_json": false,
        "disable_properties": false,
        "collapsed": false,
        "hidden": false
    },
    "defaultProperties": [
        "type",
        "id",
        "name"
    ],
    "properties": {
        "id": {
          "$ref": "#/definitions/id", 
          "propertyOrder": 10
        },
        "type": {
          "$ref": "#/definitions/type", 
          "propertyOrder": 11,
          "default": "overmap_land_use_code"
        },
        "name": {
          "$ref": "#/definitions/name", 
          "propertyOrder": 12
        },
        "land_use_code": {
            "type": "integer",
            "title": "Land Use Code",
            "default": 1,
            "propertyOrder": 30
        },
        "detailed_definition": {
            "type": "string",
            "title": "Detailed Definition",
            "default": "Generally tilled land used to grow row crops.  Boundaries follow the shape of the fields and include associated buildings (e.g., barns).  This category also includes turf farms that grow sod.",
            "format": "text",
            "propertyOrder": 50
        },
        "sym": {
            "$ref": "#/definitions/symbol", 
            "description": "The symbol for this overmap land use code",
            "propertyOrder": 13
        },
        "color": {
            "$ref": "#/definitions/color", 
            "description": "The color for this overmap land use code",
            "propertyOrder": 13
        }
    }
}